{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "newline-before-return": "error",
    "object-curly-spacing": ["error", "always"],
    "no-unused-vars": "error",
    "react/jsx-uses-vars": 1
  }
}
